<div class="form">
  <input type="text" style="display:none" class="graphVertexId" value="<%= graphVertexId || '' %>">

  <div class="resolve-wrapper">
    <div class="preview" />
    <input 
      autocomplete="off" 
      placeholder="Resolve to Entity" 
      class="object-sign"
      type="text" 
      value="<%= objectSign || sign %>">
    <span class="badge"/>
  </div>

  <div class="help">Click to show existing matches</div>

  <select style="<%= graphVertexId ? '' : 'display:none' %>">
    <option>Loading...</option>
  </select>

  <div class="visibility"></div>

  <div class="buttons">
    <button class="btn btn-link btn-small cancel">Cancel</button>
    <button style="display:none" class="btn btn-small btn-primary"><%= buttonText %></button>
  </div>
</div>
