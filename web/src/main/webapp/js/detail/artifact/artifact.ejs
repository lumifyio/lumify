
<div class="btn-toolbar">

  <%- highlightButton %>

  <div class="btn-group">
    <a class="btn btn-small dropdown-toggle" href="#" data-toggle="dropdown"><i class="icon-share"></i>
      <span class="caret"></span>
    </a>
    <ul class="dropdown-menu">
      <li><a class="" href="/artifact/<%= vertex.properties._rowKey %>/raw" target="_blank">Open <%= vertex.artifact.Generic_Metadata.fileExtension %> <%= vertex.properties._subType %></a></li>
      <li><a class="" href="/artifact/<%= vertex.properties._rowKey %>/raw?download=true" target="_blank">Download <%= vertex.artifact.Generic_Metadata.fileExtension %> <%= vertex.properties._subType %></a></li>
    </ul>
  </div>

  <%- fullscreenButton %>

  <%- auditsButton %>
</div>

<h1 class="generic-draggable artifact-title" data-vertex-id='<%= vertex.id %>'><%= vertex.properties.title %></h1>

<% if(vertex.properties._subType == 'video') { %>
  <div class="preview"/>
  <div class="currentTranscript"/>
<% } %>
<% if(vertex.properties._subType == 'image') { %>
  <div class="image-preview opens-dropdown">
    <img class="target" src='/artifact/<%= vertex.properties._rowKey %>/raw' width="auto" height="300"/>
  </div>
<% } %>

<div class='detected-object-labels'>
  <% (vertex.properties._detectedObjects || []).forEach(function(object) {
    if (object.graphVertexId) {
        var classes = 'label-info detected-object resolved entity subType-' + object._subType;%>
        <span class="label detected-object-tag">
            <a class="<%= classes %> opens-dropdown" data-info="<%= JSON.stringify(object) %>" href="#"><%= object.title || object.concept %></a><a class="delete-tag">&times;</a>
        </span>
    <%} else { %>
    <span class="label detected-object-tag">
        <a class="label-info detected-object opens-dropdown" data-info="<%= JSON.stringify(object) %>" href="#"><%= object.title || object.concept %></a>
    </span>
  <% }}); %>
</div>

<div class="artifact-header">
  <div class="properties">
    <div class="nav-header">Properties<span class="badge loading"/></div>
  </div>
</div>

<div class="text"><span class="badge loading"/></div>
